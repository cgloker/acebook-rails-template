<p id="notice"><%= notice %></p>

<h1>Friendrequests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @friendrequests.each do |friendrequest| %>
      <tr>
        <td><%= link_to 'Show', friendrequest %></td>
        <td><%= link_to 'Edit', edit_friendrequest_path(friendrequest) %></td>
        <td><%= link_to 'Destroy', friendrequest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Friendrequest', new_friendrequest_path %>
